<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://cordova.apache.org/ns/plugins/1.0" id="cordova-plugin-ios-ambient-sound" version="1.0.0">
  <name>AmbientSound</name>
  <description>AmbientSound plugin for Cordova</description>
  <license>Apache 2.0</license>
  <keywords>cordova,ambientsound</keywords>

  <!-- ios -->
  <platform name="ios">
    <config-file parent="/*" target="config.xml">
      <feature name="AmbientSound">
        <param name="onload" value="true" />
        <param name="ios-package" value="AmbientSound" />
      </feature>
    </config-file>
    <header-file src="src/ios/AmbientSound.h" />
    <source-file src="src/ios/AmbientSound.m" />
  </platform>
</plugin>
